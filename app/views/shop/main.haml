- @groups.in_groups_of(3).each do |array|
  -array.each do |group|
    .col-md-4.col-sm-6.col-xs-12
      -next unless group
      .group-wrapper
        =link_to shop_group_path(:link => group.link) do
          = image_tag group.main_picture.url(:product) , :class => "img-responsive"
          %h3= group.name
